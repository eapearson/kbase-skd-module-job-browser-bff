{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "The Root Schema",
    "required": [
        "log",
        "total_count"
    ],
    "properties": {
        "job": {
            "$ref": "base.json#definitions/job_info"
        },
        "log": {
            "type": "array",
            "title": "The Log Schema",
            "items": {
                "type": "object",
                "title": "The Items Schema",
                "required": [
                    "logged_at",
                    "message",
                    "level"
                ],
                "properties": {
                    "row": {
                        "type": "integer"
                    },
                    "logged_at": {
                        "type": "integer",
                        "title": "The Entry_number Schema"
                    },
                    "message": {
                        "type": "string",
                        "title": "The Entry Schema"
                    },
                    "level": {
                        "type": "string",
                        "title": "The Level Schema",
                        "enum": ["normal", "error"]
                    }
                }
            }
        },
        "total_count": {
            "type": "integer",
            "title": "The Total_count Schema",
            "default": 0,
            "examples": [
                100
            ]
        }
    }
}